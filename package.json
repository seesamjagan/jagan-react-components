{
  "name": "jagan-react-components",
  "version": "0.0.5",
  "private": true,
  "description": "library for web ui components build for react js",
  "keywords": [
    "react",
    "web",
    "ui",
    "component"
  ],
  "directories": {
    "lib": "lib"
  },
  "files": [
    "lib"
  ],
  "main": "./lib/index.js",
  "dependenciesForRollup": {
    "cssnano": "^4.1.10",
    "eslint-plugin-react": "^7.14.3",
    "eslint": "^6.4.0",
    "postcss-copy-assets": "^0.3.1",
    "postcss-url": "^8.0.0",
    "rollup-plugin-asset-url": "^0.1.1",
    "rollup-plugin-copy": "^3.1.0",
    "rollup-plugin-eslint": "^7.0.0",
    "rollup-plugin-url": "^2.2.2"
  },
  "devDependencies": {
    "@babel/core": "^7.6.0",
    "@babel/plugin-proposal-class-properties": "^7.5.5",
    "@babel/preset-env": "^7.6.0",
    "@babel/preset-react": "^7.0.0",
    "babel-loader": "^8.0.6",
    "clean-webpack-plugin": "^3.0.0",
    "css-loader": "^3.2.0",
    "csv-loader": "^3.0.2",
    "file-loader": "^4.2.0",
    "html-webpack-plugin": "^3.2.0",
    "jest": "^24.9.0",
    "mini-css-extract-plugin": "^0.8.0",
    "node-sass": "^4.12.0",
    "react": "^16.9.0",
    "react-dom": "^16.9.0",
    "sass-loader": "^8.0.0",
    "style-loader": "^1.0.0",
    "webpack": "^4.40.2",
    "webpack-cli": "^3.3.9",
    "webpack-dev-server": "^3.8.1",
    "webpack-node-externals": "^1.7.2",
    "xml-loader": "^1.2.1"
  },
  "peerDependencies": {
    "react": "^16.9.0",
    "react-dom": "^16.9.0"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,mjs}"
    ],
    "coverageDirectory": "coverage",
    "coverageReporters": ["text", "lcov"],
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.{js,jsx,mjs}",
      "<rootDir>/src/**/*.(spec|test).{js,jsx,mjs}"
    ],
    "transform": {
      "^.+\\.(js|jsx|mjs)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.s?css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|mjs|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    
    "testPathIgnorePatterns": [
      "<rootDir>/node_modules/",
      "<rootDir>/backup",
      "<rootDir>/demo",
      "<rootDir>/lib",
      "<rootDir>/config"
    ]
  },
  "scripts": {
    "test": "node scripts/test.js --env=jsdom",
    "test:coverage": "node scripts/test.js --env=jsdom --coverage",
    "build": "webpack --env.NODE_ENV=production",
    "watch": "webpack --watch --env.NODE_ENV=development",
    "start": "webpack-dev-server --open --config webpack.config.dev.js --env.NODE_ENV=development"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/seesamjagan/jagan-react-components.git"
  },
  "license": "MIT",
  "author": {
    "name": "சாமி. ஜெகன் லங்கா. Jagan Langa",
    "email": "seesamjagan@yahoo.co.in",
    "url": "https://github.com/seesamjagan"
  },
  "contributors": [
    {
      "name": "சாமி. ஜெகன் லங்கா. Jagan Langa",
      "email": "seesamjagan@yahoo.co.in",
      "url": "https://github.com/seesamjagan"
    }
  ],
  "bugs": {
    "url": "https://github.com/seesamjagan/jagan-react-components/issues"
  },
  "homepage": "https://github.com/seesamjagan/jagan-react-components#readme"
}
